<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidxx.yangjw.coordinatorlayoutdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".CustomBehaviorActivity" />
        <activity android:name=".AppBarActivity" />
        <activity android:name=".CollapsingToolBarLayoutActivity" />
        <activity android:name=".DetailsActivity" />
        <activity android:name=".SnackBarActivity" />
        <activity android:name=".EditTextActivity" />
        <activity android:name=".AnimActivity" />
        <activity android:name=".Anim2Activity" />
        <activity android:name=".RefreshTabActivity"></activity>
    </application>

</manifest>